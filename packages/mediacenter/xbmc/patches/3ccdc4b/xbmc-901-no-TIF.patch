From 2abc942b89a617d673df2b90d80c706b78e9bc09 Mon Sep 17 00:00:00 2001
Date: Thu, 7 Feb 2013 10:17:00 +0000
Subject: [PATCH] TIF patch

---
 lib/cximage-6.0/CxImage/DllInterface.cpp |    4 ++--
 lib/cximage-6.0/CxImage/ximacfg.h        |    2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/lib/cximage-6.0/CxImage/DllInterface.cpp b/lib/cximage-6.0/CxImage/DllInterface.cpp
index d6f8ac8..8fe57d4 100644
--- a/lib/cximage-6.0/CxImage/DllInterface.cpp
+++ b/lib/cximage-6.0/CxImage/DllInterface.cpp
@@ -56,8 +56,8 @@ DWORD GetImageType(const char *file)
   else if ( 0 == strcmpi(ext, "jpeg") )   return CXIMAGE_FORMAT_JPG;
   else if ( 0 == strcmpi(ext, "png") )    return CXIMAGE_FORMAT_PNG;
   else if ( 0 == strcmpi(ext, "ico") )    return CXIMAGE_FORMAT_ICO;
-  else if ( 0 == strcmpi(ext, "tif") )    return CXIMAGE_FORMAT_TIF;
-  else if ( 0 == strcmpi(ext, "tiff") )   return CXIMAGE_FORMAT_TIF;
+//  else if ( 0 == strcmpi(ext, "tif") )    return CXIMAGE_FORMAT_TIF;
+//  else if ( 0 == strcmpi(ext, "tiff") )   return CXIMAGE_FORMAT_TIF;
   else if ( 0 == strcmpi(ext, "tga") )    return CXIMAGE_FORMAT_TGA;
   else if ( 0 == strcmpi(ext, "pcx") )    return CXIMAGE_FORMAT_PCX;
 
diff --git a/lib/cximage-6.0/CxImage/ximacfg.h b/lib/cximage-6.0/CxImage/ximacfg.h
index 9156a5a..ee7d98d 100644
--- a/lib/cximage-6.0/CxImage/ximacfg.h
+++ b/lib/cximage-6.0/CxImage/ximacfg.h
@@ -29,7 +29,7 @@
 #define CXIMAGE_SUPPORT_JPG 1
 #define CXIMAGE_SUPPORT_PNG 1
 #define CXIMAGE_SUPPORT_ICO 1
-#define CXIMAGE_SUPPORT_TIF 1
+#define CXIMAGE_SUPPORT_TIF 0
 #define CXIMAGE_SUPPORT_TGA 1
 #define CXIMAGE_SUPPORT_PCX 1
 #define CXIMAGE_SUPPORT_WBMP 1
-- 
1.7.10.4

